{
  "name": "racer-server",
  "version": "1.0.0",
  "description": "Racer Server",
  "homepage": "https://github.com/carlocorradini/racer-server",
  "bugs": {
    "url": "https://github.com/carlocorradini/racer-server/issues",
    "email": "carlo.corradini@studenti.unitn.it"
  },
  "main": "./build/app.js",
  "private": true,
  "scripts": {
    "clean": "npx rimraf build/ && npx rimraf log/",
    "update": "npx ncu -u && npm install",
    "prettify": "npx prettier --write ./src/**/*.*",
    "prettify:staged": "npx prettier --write",
    "lint": "npx eslint --fix ./src/**/*.*",
    "lint:staged": "npx eslint --fix",
    "prebuild": "npm run clean",
    "build": "npx ttsc --p ./tsconfig.json",
    "postbuild": "npx ts-node ./script/build.ts",
    "test": "jest",
    "test:staged": "jest --bail --findRelatedTests",
    "prestart": "npm install --only=prod",
    "start": "npx cross-env NODE_ENV=\"production\" node app.js",
    "prestart:prod": "cd build && npm install --only=prod",
    "start:prod": "cd build && npx cross-env NODE_ENV=\"production\" node app.js",
    "start:dev": "npx ts-node-dev --prefer-ts true --no-notify -r tsconfig-paths/register --watch src --transpileOnly src/app.ts"
  },
  "author": "Carlo Corradini",
  "engines": {
    "node": "^10.16.0",
    "npm": "^6.9.0"
  },
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-transformer-validator": "^0.8.0",
    "class-validator": "^0.12.2",
    "cloudinary": "^1.21.0",
    "compression": "^1.7.4",
    "convert-units": "^2.3.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "envalid": "^6.0.1",
    "express": "^4.17.1",
    "express-handlebars": "^4.0.4",
    "express-jwt": "^5.3.3",
    "express-validator": "^6.5.0",
    "handlebars-helpers": "^0.10.0",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.26.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.6",
    "nodemailer-express-handlebars": "^4.0.0",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "serve-favicon": "^2.5.0",
    "typeorm": "^0.2.25",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/convert-units": "^2.3.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-handlebars": "^3.1.0",
    "@types/express-jwt": "0.0.42",
    "@types/handlebars-helpers": "^0.5.2",
    "@types/helmet": "0.0.47",
    "@types/jest": "^25.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.5",
    "@types/nodemailer": "^6.4.0",
    "@types/serve-favicon": "^2.5.0",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.6",
    "npm-check-updates": "^6.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.9.3",
    "typescript-transform-paths": "^1.1.14"
  }
}
